{
  "name": "cmlre-marine-platform",
  "version": "1.0.0",
  "description": "AI-enabled digital platform for marine data integration and analysis for CMLRE",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [
    "marine",
    "oceanography",
    "biodiversity",
    "data-integration",
    "AI",
    "CMLRE"
  ],
  "author": "CMLRE Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "sharp": "^0.32.6",
    "jimp": "^0.22.10",
    "tensorflow": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "opencv4nodejs": "^5.6.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "socket.io": "^4.7.4",
    "redis": "^4.6.12",
    "bull": "^4.12.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
